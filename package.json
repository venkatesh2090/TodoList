{
  "name": "express-example",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "bundle": "webpack",
    "dev": "NODE_ENV=development node ./dist/server.js",
    "start": "NODE_ENV=production node ./dist/server.js",
    "build": "npm-run-all bundle",
    "start-dev": "npm-run-all build dev"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.4",
    "cookie-session": "^1.4.0",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "keygrip": "^1.1.0",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "pg-promise": "^10.5.6",
    "pug": "^3.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "apply-loader": "^2.0.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.6.0",
    "fast-async": "^6.3.8",
    "file-loader": "^6.0.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
    "pug-loader": "^2.4.0",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^1.7.2"
  },
  "nodemonConfig": {
    "exec": "npm run start-dev",
    "watch": [
      "server/*",
      "public/*",
      "views/*",
      "configs/*"
    ],
    "ext": "jsx,js,json,css,html,pug,svg"
  }
}
